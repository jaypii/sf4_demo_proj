{% extends 'base.html.twig' %}

{% block title %}Read: {{ title }}{% endblock %}

{% block body %}
<div class="row">
    <div class="col-md-2">&nbsp;</div>
    <div class="col-md-8">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{title}}</h5>
                <p class="card-text">{{ articleContent }}</p>
            </div>
        </div>
    </div>
    <div class="col-md-2">&nbsp;</div>
</div>
{% endblock %}