<div class="card bg-light mt-2 p-2">
<form method="post" action="{{ path('test_ent_delete', {'id': test_ent.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ test_ent.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
</div>
